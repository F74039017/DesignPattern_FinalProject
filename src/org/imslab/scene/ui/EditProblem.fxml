<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.imslab.controller.EditController">
   <children>
      <Label fx:id="editProblemDesc" layoutX="25.0" layoutY="25.0" text="Subject_Level_No.">
         <font>
            <Font name="Verdana" size="18.0" />
         </font></Label>
      <Label layoutX="60.0" layoutY="100.0" text="Question&#10;Content " textAlignment="CENTER">
         <font>
            <Font name="Verdana" size="14.0" />
         </font></Label>
      <TextArea fx:id="problemContent" layoutX="150.0" layoutY="100.0" prefHeight="195.0" prefWidth="470.0" />
      <TextField fx:id="optionAContent" layoutX="150.0" layoutY="350.0" prefHeight="20.0" prefWidth="470.0" />
      <TextField fx:id="optionBContent" layoutX="150.0" layoutY="420.0" prefHeight="20.0" prefWidth="470.0" />
      <TextField fx:id="optionCContent" layoutX="150.0" layoutY="490.0" prefHeight="27.0" prefWidth="470.0" />
      <TextField fx:id="optionDContent" layoutX="150.0" layoutY="560.0" prefHeight="20.0" prefWidth="470.0" />
      <Label layoutX="60.0" layoutY="353.0" text="Option A">
         <font>
            <Font name="Verdana" size="14.0" />
         </font></Label>
      <Label layoutX="60.0" layoutY="423.0" text="Option B">
         <font>
            <Font name="Verdana" size="14.0" />
         </font></Label>
      <Label layoutX="60.0" layoutY="493.0" text="Option C">
         <font>
            <Font name="Verdana" size="14.0" />
         </font></Label>
      <Label layoutX="60.0" layoutY="563.0" text="Option D">
         <font>
            <Font name="Verdana" size="14.0" />
         </font></Label>
      <Button fx:id="editButton" layoutX="450.0" layoutY="630.0" mnemonicParsing="false" onAction="#processEdit" prefWidth="70.0" text="Edit">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="cancelButton" layoutX="550.0" layoutY="630.0" mnemonicParsing="false" onAction="#processCancel" prefWidth="70.0" text="Cancel">
         <font>
            <Font name="Verdana" size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
